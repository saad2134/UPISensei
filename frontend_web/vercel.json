{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "functions": {
    "app/api/chat/route.ts": {
      "maxDuration": 30,
      "memory": 1024
    },
    "app/api/upload/route.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key"
  }
}